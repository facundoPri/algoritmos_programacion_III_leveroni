	| opinionGeneral |

	opinionGeneral := seguidores inject: 0 into: [:valor :opinionDePueblo |
		(opinionDePueblo esVistoComoPrometedor: unPersonajeJugador)
		ifTrue:[
			valor+1
		]
		ifFalse:[
			valor-1
		]
	].
	(opinionGeneral <= 0)
	ifTrue:[
		^self opinionDesconocido 
	]
	ifFalse:[
		^self opinionPrometedor 
	]! !
